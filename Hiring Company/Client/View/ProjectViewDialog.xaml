<Window x:Class="Client.View.ProjectViewDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client.View"
        xmlns:userctrl="clr-namespace:Common.UserControls;assembly=Common"
        mc:Ignorable="d"
        Title="ProjectViewDialog" Height="400" Width="400"
        Name="root">
    <Grid>
        <userctrl:CreateProjectView SaveClicked="CreateProjectView_SaveClicked" CancelClicked="CreateProjectView_CancelClicked" Project="{Binding ElementName=root,Path=Project}"></userctrl:CreateProjectView>
    </Grid>
</Window>
